{
  "name": "angular-modules",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:stats": "ng build --stats-json",
    "analyze": "webpack-bundle-analyzer dist/angular-modules/stats.json",
